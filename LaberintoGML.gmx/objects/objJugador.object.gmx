   if (!place_meeting(x-velocidad,y,objMuro))&amp;&amp;(!place_meeting(x-velocidad,y,objPuertaPadre))
   {x=x-velocidad;}
   }
if (keyboard_check(vk_right))
   {
   if (!place_meeting(x+velocidad,y,objMuro))&amp;&amp;(!place_meeting(x-velocidad,y,objPuertaPadre))
   {x=x+velocidad;}
